<div class="row">
	<div class="col-lg-12 col-md-12">
		<h4>Users:</h4>
		<table class="table table-striped">
			<tr>
				<th>
					username:
				</th>      
				<th>
					_id:
				</th>
				<th>
					password:
				</th>
				<th>
					email:
				</th>
				<th>
					joinDate:
				</th>
				<th>
					version:
				</th>
				<th>
					lastAction:
				</th>
			</tr>
			{{#each users }}
			<tr>
				<td>
				{{ this.username }}
				</td>
				<td>
				{{ this._id }}
				</td>
				<td>
				{{ this.password }}
				</td>
				<td>
				{{ this.email }}
				</td>
				<td>
				{{ this.joinDate }}
				</td>
				<td>
				{{ this.version }}
				</td>
				<td>
				{{ this.lastAction }}
				</td>
			</tr>
			{{/each }}
		</table>
	</div>
</div>